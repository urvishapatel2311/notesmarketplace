@model NotesMarketPlace.Models.tblManageCountry

@{
    Layout = "~/Views/Shared/_AdminAfterLoginLayout.cshtml";

    ViewBag.Title = "AddCountry";
}

<section id="addcountry" class="fixheight">
    <div class="content-box-md">
        <div class="container">
            <!-- General heading -->
            <div class="general-heading">
                <h3>Add Country</h3>
            </div>
            <div class="admin-p">
                <div class="row">
                    <div class="col-md-6 col-lg-6 col-sm-12 col-xl-8">
                        <form method="post">
                            <div class="form-content">
                                <div class="form-group">
                                    <label for="coname">Country Name *</label>
                                    @Html.TextBoxFor(m => m.countryName, new { placeholder = "Enter your country name", @class = "form-control", @id = "coname" })
                                    @Html.ValidationMessageFor(model => model.countryName, "", new { @class = "text-danger" })
                                </div>
                                <div class="form-group">
                                    <label for="cocode">Country Code *</label>
                                    @Html.TextBoxFor(m => m.countryCode, new { placeholder = "Enter your country code", @class = "form-control", @id = "cocode" })
                                    @Html.ValidationMessageFor(model => model.countryCode, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <button id="btn-admin-p" type="submit" class="btn btn-primary btn-block" name="command" value="update">Submit</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

